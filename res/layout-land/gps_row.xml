<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
 android:layout_height="wrap_content" android:layout_width="fill_parent"
 android:id="@+id/row" android:background="@color/not_uploaded">
  <TextView android:id="@+id/timestamp" android:layout_weight="2" android:layout_height="fill_parent" android:layout_width="fill_parent" android:width="0px" android:text="A"/>
  <TextView android:id="@+id/latitude"  android:layout_weight="3" android:layout_height="fill_parent" android:layout_width="fill_parent" android:width="0px" android:text="B"/>
  <TextView android:id="@+id/longitude" android:layout_weight="3" android:layout_height="fill_parent" android:layout_width="fill_parent" android:width="0px" android:text="C"/>
  <TextView android:id="@+id/altitude"  android:layout_weight="3" android:layout_height="fill_parent" android:layout_width="fill_parent" android:width="0px" android:text="D"/>
  <TextView android:id="@+id/accuracy"  android:layout_weight="3" android:layout_height="fill_parent" android:layout_width="fill_parent" android:width="0px" android:text="E"/>
</LinearLayout>
